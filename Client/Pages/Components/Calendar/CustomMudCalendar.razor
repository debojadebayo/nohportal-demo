@namespace Client.Pages.Components.Calendar

@using Heron.MudCalendar;
@using Client.Pages.Components.Calendar.CustomWeekView;
@using Client.Pages.Components.Calendar.CustomWorkWeekView;
@using Client.Pages.Components.Calendar.CustomMonthView;
@using Client.Pages.Components.Calendar.CustomDayView;

@inherits MudCalendar

@Render

@code {
    protected override RenderFragment RenderWeekView =>
        @<CustomWeekView />;
    protected override RenderFragment RenderWorkWeekView =>
        @<CustomWorkWeekView />;
    protected override RenderFragment RenderMonthView =>
        @<CustomMonthView />;
    protected override RenderFragment RenderDayView =>
        @<CustomDayView />;
}