<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <RunConfiguration>
    <MaxCpuCount>0</MaxCpuCount>
  </RunConfiguration>
  <NUnit>
    <NumberOfTestWorkers>4</NumberOfTestWorkers>
  </NUnit>
  <Playwright>
    <BrowserName>chromium</BrowserName>
    <ExpectTimeout>30000</ExpectTimeout>
    <LaunchOptions>
      <Headless>true</Headless>
      <SlowMo>0</SlowMo>
    </LaunchOptions>
    <Fullyparallel>true</Fullyparallel>
    <Workers>4</Workers>
  </Playwright>
  <TestRunParameters>
    <Parameter name="BaseUrl" value="https://localhost:7001" />
    <Parameter name="Timeout" value="30000" />
  </TestRunParameters>
</RunSettings>
