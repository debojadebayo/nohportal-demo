{
  "ConnectionStrings": {
    "DefaultConnection": "",
    "AzureBlobStorage": "",
    "Redis": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Warning",
      "Microsoft.AspNetCore.Authorization": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "Cors": {
    "AllowedClientOrigin": "",
    "AllowedServerOrigin": "",
    "AllowedMethods": "GET, PUT, POST, DELETE, OPTIONS",
    "AllowedHeaders": "Content-Type, Authorization, X-Requested-With",
    "ExposedHeaders": "X-Total-Count, X-Pagination",
    "MaxAge": 3600
  },
  "AllowedHosts": "*",
  "Jwt": {
    "MetadataAddress": "",
    "Issuer": "",
    "Audience": "nationoh_webapi",
    "RequireHttpsMetadata": true,
    "ClockSkew": "00:05:00",
    "TokenValidationParameters": {
      "ValidateIssuer": true,
      "ValidateAudience": true,
      "ValidateLifetime": true,
      "ValidateIssuerSigningKey": true,
      "ClockSkew": "00:05:00"
    }
  },
  "KeycloakAdminClient": {
    "KeycloakUrl": "",
    "Realm": "NationOH",
    "ClientId": "admin-cli",
    "ClientSecret": ""
  },
  "Modules": [
    "Billing",
    "CRM",
    "Clinical",
    "Schedule"
  ],
  "AuthModule": {
    "BaseUrl": ""
  },
  "Xero": {
    "ApiUrl": "https://api.xero.com/api.xro/2.0",
    "AccessToken": "",
    "TenantId": "",
    "ClientId": "",
    "ClientSecret": ""
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "KeyVault": {
    "VaultUri": "",
    "ClientId": "",
    "ClientSecret": "",
    "TenantId": ""
  },
  "Security": {
    "RequireHttps": true,
    "MaxRequestBodySize": 10485760,
    "RequestTimeout": "00:02:00",
    "EnableDataProtection": true,
    "EnableAntiforgery": true
  },
  "Performance": {
    "EnableResponseCompression": true,
    "EnableResponseCaching": true,
    "DefaultCacheDuration": 300,
    "MaxConcurrentRequests": 100,
    "RequestQueueLimit": 1000
  },
  "HealthChecks": {
    "Enabled": true,
    "DetailedErrors": false,
    "Endpoints": {
      "Health": "/health",
      "Ready": "/health/ready",
      "Live": "/health/live"
    }
  },
  "Scalar": {
    "Enabled": false,
    "RoutePrefix": "api-docs",
    "Title": "NationOH API",
    "Version": "v1",
    "Description": "NationOH Web API"
  },
  "RateLimiting": {
    "EnableRateLimiting": true,
    "GlobalLimiter": {
      "PermitLimit": 1000,
      "Window": "00:01:00",
      "QueueLimit": 100
    },
    "Policies": {
      "ApiPolicy": {
        "PermitLimit": 100,
        "Window": "00:01:00",
        "QueueLimit": 10
      }
    }
  }
}
